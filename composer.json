{
    "name": "softspring/translatable-bundle",
    "description": "Symfony form type for translatable fields",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "doctrine/dbal": "^4.2",
        "doctrine/orm": "^3.3",
        "symfony/doctrine-bridge": "^7.1",
        "symfony/config": "^5.4|^6.0|^7.0",
        "symfony/dependency-injection": "^5.4|^6.0|^7.0",
        "symfony/form": "^5.4|^6.0|^7.0",
        "symfony/http-kernel": "^5.4|^6.0|^7.0",
        "twig/twig": "^3.14"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.64.*",
        "google/cloud-translate": "^1.17",
        "phpstan/phpstan": "^1.10",
        "rector/rector": "^1.0",
        "softspring/dynamic-form-type": "^5.4",
        "symfony/framework-bundle": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\TranslatableBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
